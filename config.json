{
  "evaluators": {
    "codeBlock": {
      "sh": {
        "path": "/usr/bin/env",
        "defaultArguments": ["sh"]
      },
      "python": {
        "path": "/usr/bin/env",
        "defaultArguments": ["python3"],
        "session": {
          "command": ["python3", "-i"]
        }
      },
      "diagon": {
        "path": "nix",
        "defaultArguments": [
          "--extra-experimental-features", "nix-command",
          "--extra-experimental-features", "flakes",
          "run", "github:petertrotman/nixpkgs/Diagon#diagon",
          "--", "{mode}", "-style=Unicode"
        ]
      },
      "openscad": {
        "path": "sh",
        "defaultArguments": [
          "-c",
          "xvfb-run -a openscad -o {output} --colorscheme '{colorscheme}' --imgsize {size} --projection o --viewall {input_file} && magick {output} -transparent '#1D1F21' {output}"
        ],
        "inputExtension": ".scad",
        "defaultParams": {
          "colorscheme": "Tomorrow Night",
          "size": "400,400"
        }
      },
      "node": {
        "path": "/usr/bin/env",
        "defaultArguments": ["node"],
        "session": {
          "command": ["node"]
        }
      },
      "dot": {
        "path": "dot",
        "defaultArguments": [
          "-Tsvg",
          "-o", "{output}"
        ],
        "prefix": "digraph { rankdir=LR; bgcolor=transparent; node [shape=egg height=\"0\" margin=\"0.1,0.05\" fontname=terminus fontsize=13 fontcolor=white color=\"#ffffff99\" style=\"solid\"] edge [color=\"#ffffff\" fillcolor=\"transparent\" style=\"solid\"] ",
        "suffix": "}"
      }
    }
  }
}
